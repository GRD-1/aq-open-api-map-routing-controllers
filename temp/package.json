        "build-storybook": "storybook build",
        "build-storybook:ci": "doppler run --config prod -- storybook build",
        "chromatic": "doppler run --config prod -- npx chromatic",
        "chromatic:ci": "doppler run --config prod -- npx chromatic --exit-zero-on-changes --auto-accept-changes",
        "postinstall": "scripts/postinstall.sh",
        "audit:fix": "doppler run --config dev -- npm audit fix",
        "setup": "doppler run --project app --config dev -- npm i"
