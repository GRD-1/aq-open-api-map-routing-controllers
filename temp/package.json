        "mock:start:config-service": "prism mock -d -h localhost -p 5560 ./mocks/config-service/schema.json",
        "serve:staging": "doppler run -- npm run front:e2e:build && DEPLOYED_ENVIRONMENT=true npm run back:start staging",
        "serve:ci": "ts-node --project tsconfig.server.json ./server/start",
        "migrate:up": "./scripts/migrate.sh up",
        "migrate:down": "./scripts/migrate.sh down",
        "migrate:development:drop": "NODE_ENV=development npx sequelize-cli db:migrate:undo:all",
        "migrate:test:up": "npx sequelize-cli db:migrate",
        "migrate:test:down": "npx sequelize-cli db:migrate:undo",
        "migrate:test:drop": "npx sequelize-cli db:migrate:undo:all",
        "migration:create": "npx sequelize-cli migration:generate --name $1",
        "migrate:controllers": "ts-node --project tsconfig.server.json ./migrators/index.ts",
        "seed:up": "./scripts/seed.sh up",
        "seed:down": "./scripts/seed.sh down",
        "test:integration": "doppler run --project app --config dev_test_local -- ./scripts/test.sh integration",
        "test:integration:ci": "./scripts/test.sh ci",
        "test:integration:debug": "doppler run --config dev_test_local -- ./scripts/test.sh integration:debug",
        "test:playwright": "doppler run --config dev_test_local -- ./scripts/test.sh playwright",
        "test:playwright:debug": "doppler run --config dev_test_local -- ./scripts/test.sh playwright:debug",
        "test:playwright:ci": "./scripts/test.sh playwright:ci",
        "test:e2e": "doppler run --project app --config dev_test_local -- ./scripts/test.sh $1",
        "test:unit": "doppler run --project app --config dev_test_local -- ./scripts/test.sh vitest",
        "test:unit:ui": "doppler run --project app --config dev_test_local -- ./scripts/test.sh vitest:coverage --ui",
