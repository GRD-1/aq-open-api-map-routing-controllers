{
  "name": "aq-open-api-map-routing-controllers",
  "version": "1.0.0",
  "description": "Open API Map Routing Controllers",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:down:all": "npx sequelize-cli db:migrate:undo:all",
    "run": "npm run",
    "generate:openapi": "ts-node src/openapi/generate.ts"
  },
  "dependencies": {
    "@types/pino": "^7.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "class-validator-jsonschema": "^5.0.2",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "pg": "^8.15.6",
    "pg-hstore": "^2.3.4",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "reef-framework": "^0.0.39",
    "routing-controllers-openapi": "^5.0.0",
    "sequelize": "^6.37.7",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "sequelize-cli": "^6.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
